class Solution {
    public boolean isRectangleOverlap(int[] rec1, int[] rec2) {
        int x0 = rec1[0], y0 =rec1[1], x1 = rec1[2], y1=rec1[3];
        int a0 = rec2[0], b0 =rec2[1], a1 = rec2[2], b1=rec2[3];
        
        if(y0>=b1 || x1<=a0 || b0>=y1 || a1<=x0) return false;
        return true;
        
    }
}
