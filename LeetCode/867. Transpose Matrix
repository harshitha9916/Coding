class Solution {
    public int[][] transpose(int[][] matrix) {
        int r = matrix.length, c = matrix[0].length;
        int[][] n = new int[c][r];
        for(int i=0;i<r;i++){
            for(int j=0;j<c;j++){
                n[j][i] = matrix[i][j];  
            }
        }
        return n;
    }
}
