class Solution {
    
    //Using Sorting Method
    
    public String longestCommonPrefix(String[] strs) {
        
        Arrays.sort(strs);
        int n = strs.length;
        
        int min = Math.min(strs[0].length(),strs[n-1].length());
        String res = "";
        
        for(int i=0;i<min;i++){
            if(strs[0].charAt(i) != strs[n-1].charAt(i)){
                return res;
            }
            res+=strs[0].charAt(i);
        }
        return res;
    }
}
